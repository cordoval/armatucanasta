{% extends 'GicsdrArmatucanastaBundle::base.html.twig' %}

{% block title %}Crear una conversión{% endblock %}

{% block body %}
<h1>Crear una conversión</h1>

<form action="{{ path('conversion_create') }}" method="post" {{ form_enctype(form) }}>
    {{ form_widget(form) }}
    <p>
        <button type="submit">Crear</button>
    </p>
</form>

<ul class="record_actions">
    <li>
        <a href="{{ path('conversion') }}">
            Volver a la lista
        </a>
    </li>
</ul>
{% endblock %}
