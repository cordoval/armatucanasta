<h1>Lista de Insumos</h1>

<table class="records_list">
    <thead>
        <tr>
            <th>Id_unidad</th>
            <th>Codigo</th>
            <th>Descripcion</th>
            <th>Actions</th>
        </tr>
    </thead>
    <tbody>
    {% for entity in entities %}
        <tr>
            <td><a href="{{ path('insumo_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
            <td>{{ entity.idunidad }}</td>
            <td>{{ entity.codigo }}</td>
            <td>{{ entity.descripcion }}</td>
            <td>
                <ul>
                    <li>
                        <a href="{{ path('insumo_show', { 'id': entity.id }) }}">Mostrar</a>
                    </li>
                    <li>
                        <a href="{{ path('insumo_edit', { 'id': entity.id }) }}">Editar</a>
                    </li>
                </ul>
            </td>
        </tr>
    {% endfor %}
    </tbody>
</table>

<ul>
    <li>
        <a href="{{ path('insumo_new') }}">
            Crear nueva lista de insumos
        </a>
    </li>
</ul>
